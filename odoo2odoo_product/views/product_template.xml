<openerp>
<data>

<record id="product_template_form_view" model="ir.ui.view">
    <field name="name">product.template.form.inherit</field>
    <field name="model">product.template</field>
    <field name="inherit_id" ref="connector_base_product.product_template_common_form"/>
    <field name="arch" type="xml">
        <page name="connector" position="attributes">
            <attribute name="invisible">0</attribute>
        </page>
        <page name="connector" position="inside">
            <group name="odoo2odoo" string="Odoo2Odoo" groups="connector.group_connector_manager">
                <field name="odoo_bind_ids" nolabel="1"/>
            </group>
        </page>
        <xpath expr="//div[@name='options']" position="inside">
          <div>
              <field name="no_export_tmpl"/>
              <label for="no_export_tmpl"/>
          </div>
        </xpath>
    </field>
</record>

</data>
</openerp>
