<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data>

<record id="product_pricelist_form_view" model="ir.ui.view">
    <field name="name">product.pricelist.connector</field>
    <field name="model">product.pricelist</field>
    <field name="inherit_id" ref="product.product_pricelist_view"/>
    <field name="arch" type="xml">
        <form position="inside">
          <group name="odoo2odoo" string="Odoo2Odoo" groups="connector.group_connector_manager">
              <group col="4">
                  <field name="no_export"/>
              </group>
              <group colspan="4">
                  <field name="odoo_bind_ids" nolabel="1"/>
              </group>
          </group>
        </form>
        <xpath expr="//field[@name='version_id']/form//field[@name='date_start']" position="after">
            <field name="no_export" groups="connector.group_connector_manager"/>
        </xpath>
    </field>
</record>

</data>
</openerp>
