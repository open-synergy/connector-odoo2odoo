<openerp>
<data>

  <record id="view_odooconnector_product_template_form" model="ir.ui.view">
      <field name="name">odooconnector.product.product.form</field>
      <field name="model">product.product</field>
      <field name="inherit_id" ref="product.product_normal_form_view"/>
      <field name="arch" type="xml">
        <xpath expr="//div[@name='options']" position="inside">
          <div>
              <field name="no_export_product"/>
              <label for="no_export_product"/>
          </div>
        </xpath>
      </field>
  </record>

  <record id="odoo_product_product_view_tree" model="ir.ui.view">
      <field name="name">odoo.product.product.tree</field>
      <field name="model">odoo.product.product</field>
      <field name="arch" type="xml">
          <tree string="Odoo2Odoo Bindings">
              <field name="backend_id"/>
              <field name="odoo_id"/>
              <field name="external_odoo_id"/>
              <field name="sync_date"/>
          </tree>
      </field>
  </record>

</data>
</openerp>
